{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/ui/canvas-reveal-effect.jsx",
    "src/components/ui/wavy-background.jsx",
    "src/components/ui/text-generate-effect.jsx",
    "src/components/Navbar/Logo.jsx",
    "src/components/ui/Image-slider.jsx",
    "src/components/Hero/imageslider.jsx",
    "src/components/Projek/projek.jsx"
  ],
  "exclude": ["node_modules"]
}
